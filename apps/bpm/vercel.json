{
  "buildCommand": "cd ../../ && pnpm install && pnpm --filter @workspace/bpm build",
  "outputDirectory": "dist",
  "installCommand": "cd ../../ && pnpm install",
  "framework": null,
  "devCommand": "pnpm --filter @workspace/bpm dev",
  "headers": [
    {
      "source": "/mf-manifest.json",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
